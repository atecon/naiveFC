function void test_diff_t2_of_y_and_x (void)
    print "Start with diff_t2_of_y_and_x()"

    series s1 = normal()
    series s2 = s1
    series s3 = s1
    series s4 = s1
    s2[1] = NA
    s3[7] = NA
    s4[1] = NA
    s4[7] = NA

    list L = s1
    scalar s = diff_t2_of_y_and_x(s1, L)
    assert_equal_num(s, 0)

    list L = s2
    scalar s = diff_t2_of_y_and_x(s1, L)
    assert_equal_num(s, 0)

    list L = s3
    scalar s = diff_t2_of_y_and_x(s1, L)
    assert_equal_num(s, -1)

    list L = s4
    scalar s = diff_t2_of_y_and_x(s1, L)
    assert_equal_num(s, -1)

    list L = s4
    scalar s = diff_t2_of_y_and_x(s3, L)
    assert_equal_num(s, 0)
end function
test_diff_t2_of_y_and_x()



